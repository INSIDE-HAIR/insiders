// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

// Import all schema files
import "access.prisma"
import "auth.prisma"
import "codes.prisma"
import "contactBackup.prisma"
import "documents.prisma"
import "drive.prisma"
import "fields.prisma"
import "forms.prisma"
import "holded.prisma"
import "media.prisma"
import "page.prisma"
import "products.prisma"
import "services.prisma"
import "user.prisma"
